#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules/vscode-languageserver/bin/node_modules:/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules/vscode-languageserver/node_modules:/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules:/mnt/c/Users/pc/Downloads/php_project/node_modules:/mnt/c/Users/pc/Downloads/node_modules:/mnt/c/Users/pc/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules/vscode-languageserver/bin/node_modules:/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules/vscode-languageserver/node_modules:/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules:/mnt/c/Users/pc/Downloads/php_project/node_modules:/mnt/c/Users/pc/Downloads/node_modules:/mnt/c/Users/pc/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/pc/Downloads/php_project/absence-management-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
